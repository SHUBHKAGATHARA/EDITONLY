@{
    ViewData["Title"] = "ProfileSetting";
}

<div class="container mt-4">
    <h2 class="mb-3">Profile Settings</h2>
    <dl class="row">
        <dt class="col-sm-3">Username:</dt>
        <dd class="col-sm-9">@ViewBag.UserName</dd>

        <dt class="col-sm-3">Email:</dt>
        <dd class="col-sm-9">@ViewBag.Email</dd>

        <dt class="col-sm-3">Role:</dt>
        <dd class="col-sm-9">@ViewBag.Role</dd>
    </dl>
</div>
